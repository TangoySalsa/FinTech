  <log4net xsi:noNamespaceSchemaLocation="http://csharptest.net/downloads/schema/log4net.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" debug="true">

    <root>
      <level value="ALL" />
      <appender-ref ref="RollingFileAppender" />
      <!-- <level value="ERROR" />-->
    </root>

    <appender name="RollingFileAppender" type="log4net.Appender.RollingFileAppender">
      <staticLogFileName value="false" />
      <file value="Logs\LogFile" />
      <appendToFile value="true" />
      <rollingStyle value="Date" />
      <datePattern value="&quot;.&quot;yyyy-MM-dd&quot;.log&quot;" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date{HH:mm:ss.fff} %5level [%thread] - %message%newline" />
      </layout>
      <filter type="log4net.Filter.LevelRangeFilter">
        <levelMin value="DEBUG" />
        <levelMax value="FATAL" />
      </filter>
    </appender>
    <!-- del minimo al máximo: DEBUG, INFO, WARN, ERROR, FATAL -->

  </log4net>




